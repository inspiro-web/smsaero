{
  "name": "inspiro/smsaero",
  "description": "Library to interact with SMSAero API",
  "type": "library",
  "license": "MIT",
  "authors": [{
    "name": "Inspiro Web",
    "email": "mail@inspiro.site"
  }],
  "require": {
    "php" : "^7.2.5",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "^7.0"
  },
  "require-dev": {
    "laravel/framework": "^7.0"
  },
  "autoload": {
      "psr-4": {
          "SMSAero\\": "src/"
      }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "SMSAero\\SMSAeroServiceProvider"
      ],
      "aliases": {
        "SMSAero": "SMSAero\\SMSAero"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
